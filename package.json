  "scripts": {
    "backtest": "TZ=America/New_York node dist/bin/backtest.js",
    "test": "is-ci-cli test:coverage test:watch",
    "test:coverage": "TZ=America/New_York jest --coverage",
    "test:watch": "TZ=America/New_York jest --watch",
    "check": "tsc --noEmit",
    "check:watch": "tsc --noEmit --watch",
    "build": "rm -rf ./dist && npx parcel build",
    "build:watch": "npx parcel watch",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "server": "TZ=America/New_York nodemon ./dist/bin/server.js",
    "server:dev": "TZ=America/New_York nodemon ./dist/bin/server.js"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --cache --fix"
    ],
    "*.{scss}": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ],
    "*.js": "eslint --cache --fix"
  },
  "targets": {
    "frontend-backtest": {
      "source": "./src/web/backtest/index.html",
      "distDir": "./dist/public/backtest",
      "publicUrl": "/backtest"
    },
    "server": {
      "source": "./src/bin/server.ts",
      "distDir": "./dist/bin",
      "context": "node",
      "engines": {
        "node": ">=12.x"
      }
    },
    "backtest-controller": {
      "source": "./src/bin/backtest.ts",
      "distDir": "./dist/bin",
      "context": "node",
      "engines": {
        "node": ">=12.x"
      }
    },
    "backtest-worker": {
      "source": "./src/bin/worker.ts",
      "distDir": "./dist/bin",
      "context": "node",
      "engines": {
        "node": ">=12.x"
      }
    },
    "strategies": {
      "source": [
        "./src/strategies/hod.ts"
      ],
      "distDir": "./strategies",
      "context": "node",
      "engines": {
        "node": ">=12.x"
      }
    }
